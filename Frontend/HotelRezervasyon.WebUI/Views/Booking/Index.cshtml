
@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<!--Head Kısmı Başlangıç-->
@await Component.InvokeAsync("_HeadComponent")
<!--Head Kısmı Bitiş-->
<body>
    <div class="container-xxl bg-white p-0">
        <!-- Spinner Start -->
        @await Component.InvokeAsync("_SpinnerComponent")
        <!-- Spinner End -->
        <!-- Header Menü Start -->
        @await Component.InvokeAsync("_NavbarComponent")
        <!-- Header Menü End -->
         <!-- Page Header Start -->
         @await Component.InvokeAsync("_BookingCoverPartial")
        <!-- Page Header End -->
        @await Html.PartialAsync("/Views/Booking/AddBooking.cshtml")
        <!-- Subscribe Start -->
        @await Html.PartialAsync("/Views/Default/_SubscribePartial.cshtml")

        @*@await Component.InvokeAsync("_SubscribeComponent")*@
        <!-- Subscribe Start -->
        <!-- Footer Start -->
        @await Component.InvokeAsync("_FooterComponent")
        <!-- Footer End -->
        <!-- Back to Top -->
        <a href="#" class="btn btn-lg btn-primary btn-lg-square back-to-top"><i class="bi bi-arrow-up"></i></a>
    </div>
    @await Component.InvokeAsync("_ScriptComponent")
</body>
</html>